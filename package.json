{
  "name": "n8n-nodes-rvf-llm",
  "version": "1.0.3",
  "description": "RVF LLM nodes for n8n: Universal AI Gateway integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Xrey995/n8n-nodes-rvf-llm",
  "author": {
    "name": "Ruslan",
    "email": "info@rvlfreellm.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Xrey995/n8n-nodes-rvf-llm.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write nodes credentials",
    "lint": "eslint nodes credentials --ext .js,.ts",
    "lint:fix": "eslint nodes credentials --ext .js,.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RvfLLM.credentials.js"
    ],
    "nodes": [
      "dist/nodes/RvfLLM/RvfLLM.node.js",
      "dist/nodes/RvfLLMChatModel/RvfLLMChatModel.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@langchain/core": "^0.1.55",
    "axios": "^0.21.4"
  }
}
